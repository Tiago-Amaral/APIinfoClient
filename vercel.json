{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/server.js"
      }
    ],
    "env": {
      "DATABASE_URL": "@my-database-url",
      "SECRET_KEY": "@my-secret-key"
    },
    "functions": {
      "server.js": {
        "memory": 1024,
        "maxDuration": 10
      }
    }
  }
  